/* 清除浮动 */
/*  */
.clearfix:after {
  content: '';
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
  overflow: hidden;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

image {
  vertical-align: center;
}
.hide{
  display: none;
}
.show{
  display: block;
}
.fuhao{
   font-size: 30rpx;
}

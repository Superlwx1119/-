  position: fixed;
  width: 100vw;
  background: #F5F5F5;
  box-sizing: border-box;
  padding: 0 32rpx;
  overflow: hidden;
}
.gun-show {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  height: 52rpx;
  line-height: 52rpx;
  display: flex;
  position: relative;
  overflow: hidden;
}
.larg-d  .gun-box-icon {
  width: 56rpx;
  height: 52rpx;
  background:#F5F5F5 ;
  vertical-align: middle;
}
.larg-d  .gun-box-icon::after{
  content: '';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.larg-d  .gun-box-icon image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}
.larg-d .dog-d-run{
  line-height: 52rpx;
}
.fix-out {
  opacity: 0;
}
.my-menu {
  position: fixed;
  top:  calc(90px + 52rpx);
  width: 176rpx;
  background: #fff;
  z-index: 1;
  box-sizing: border-box;
  padding-top: 2vw;

}




.box{
  background:rgba(245,245,245,1);
  padding-top:40rpx;
  box-sizing: border-box;
  padding-bottom: 88rpx;
  /* height: 100vh; */
}
.sendType{
  background:rgba(255,255,255,1);
  box-shadow:0rpx 6rpx 12rpx rgba(204,204,204,0.2);
  border-radius:16rpx;
  margin: 0rpx 32rpx 24rpx 32rpx;
  padding: 24rpx 28rpx;
}
.shopSend image{
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.goodsList{
  background:rgba(255,255,255,1);
}
.shopName{
  font-size:40rpx;
  font-family:PingFang TC;
  font-weight:600;
  line-height:56rpx;
  color:rgba(51,51,51,1);
}
.shopHi{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:40rpx;
  color:rgba(102,102,102,1);
}
.shopSend{
  display: flex;
  justify-content: space-between;
  margin:48rpx 0;
}
.shopSend view{
  width:264rpx;
  height:88rpx;
  border:1px solid rgba(153,153,153,1);
  border-radius:46rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:40rpx;
  color:rgba(153,153,153,1);
}
.sendSelected{
  background:rgba(87,214,121,1) !important;
  color:rgba(255,255,255,1) !important;
  border:1px solid rgba(87,214,121,1) !important;
}
.address{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.address>text{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  color:rgba(51,51,51,1);
}
.addAddress{
  display: flex;
  align-items: center;
}
.addAddress>text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:34rpx;
  color:rgba(153,153,153,1);
  display: inline-block;
  padding-right: 30rpx;
}
.addAddress input{
  text-align: right;
}
.addAddress button{
  padding: 4rpx 0;
  width:120rpx !important;
  height:40rpx !important;
  white-space: nowrap;
  border:1px solid rgba(168,230,112,1);
  opacity:1;
  border-radius:6rpx;
  font-size:20rpx !important;
  font-family:PingFang SC;
  font-weight:400;
  line-height:28rpx !important;
  color:rgba(168,230,112,1);
}
.goodsList{
  background:rgba(255,255,255,1);
  box-shadow:0px 6rpx 12rpx rgba(204,204,204,0.2);
  border-radius:16rpx;
  padding: 24rpx 28rpx;
  margin:0 32rpx;
}
.goodsList>text{
  height:40rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  margin-bottom: 24rpx;
  color:rgba(51,51,51,1);
}
.listItem{
  display: flex;
  margin: 12rpx 0;
}
.listItem>text{
  width: 90rpx;
  height:34rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  display: inline-block;
  text-align: center;
  line-height:34rpx;
  color:rgba(102,102,102,1);
}
.manJian{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.jianqian{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:34rpx;
  color:rgba(255,86,66,1) !important;
}
.name{
  flex: 1;
  min-width: 236rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: left !important;
}
.subtotal{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-top: 60rpx;
}
.num{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.text{
  height:34rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:34rpx;
  color:rgba(102,102,102,1);
  margin: 0 10rpx;
  display: inline-block;

}
.money{
  height:34rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:34rpx;
  color:rgba(255,86,66,1);
}
.message text{
  margin-bottom: 0 !important;
  width: 25%;
}
.message input{
  font-size: 24rpx;
  color:#333333;
  flex: 1;
}
.message textarea{
  font-size: 24rpx;
  color:#333333;
  flex: 1;
  padding-top: 6rpx;
}
.message{
  margin: 24rpx 32rpx;
  display: flex;
  flex-direction: row !important;
  justify-content: space-between;
}
.selectCard{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 12rpx 0;
  margin-bottom: 24rpx;
}
.cardText{
  display: flex;
  align-items: center;
}
.cardText text{
  height:40rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  color:rgba(51,51,51,1);
}
.cardText image{
  width: 32rpx;
  height: 32rpx;
  margin: 4rpx;
}
.card{
  display: flex;
  align-items: center;
  padding-left: 50rpx;
}
.tips{
  margin-top: 10rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:28rpx;
  color:rgba(255,86,66,1) !important;
}
.sendF{
  text-align: right !important;
}
.card text{
  height:28rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:28rpx;
  color:rgba(153,153,153,1);
}
.card image{
  width: 20rpx;
  height: 20rpx;
}
.salesPrice{
  display: flex;
  margin-top: 60rpx;
  justify-content: flex-end;
  height:34rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:34rpx;
  color:rgba(102,102,102,1);
}
.salesPrice text{
  height:34rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:34rpx;
  color:rgba(255,86,66,1);
}
.total{
  display: flex;
  justify-content: space-between;
  height: 88rpx;
  background: white;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  align-items: center;
}
.total text{
  flex: 1;
  height:56rpx;
  font-size:40rpx;
  font-family:PingFang SC;
  font-weight:600;
  line-height:56rpx;
  color:rgba(51,51,51,1);
  text-indent: 64rpx;
}
.total view{
  width:220rpx;
  height:88rpx;
  background:rgba(87,214,121,1);
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:44rpx;
  color:rgba(255,255,255,1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.coupon{
  width: 590rpx;
  margin-left: 40rpx;
  height: 150rpx;
  position: relative;
}
.coupon image{
  width: 100%;
  height: 100%;
}
.oldPrice{
  font-size:20rpx !important;
  font-family:PingFang SC;
  font-weight:400;
  line-height:22rpx;
  color:rgba(204,204,204,1);
  text-decoration: line-through;
  text-indent: 1em;
}
.moneyAndName{
  position: absolute;
  top: 28rpx;
  left: 32rpx;
  height:40rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  color:rgba(255,151,73,1);
}
.moneyAndName .num{
  font-size:44rpx;
  font-family:PingFang HK;
  font-weight:500;
  line-height:60rpx;
  color:rgba(255,151,73,1);
  margin-right: 10rpx;
}
.currentCou{
  width: 40rpx !important;
  height: 40rpx !important;
  position: absolute;
  left: -40rpx;
  top: 50%;
  transform: translateY(-50%);
}
.time{
  height:28rpx;
  position: absolute;
  bottom: 36rpx;
  left: 32rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:28rpx;
  color:rgba(255,151,73,1);
}
.getIt{
  position: absolute;
  right: 50rpx;
  top: 56rpx;
  height:40rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  color:rgba(255,151,73,1);
}
.hasAddress{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.hasAddress image{
  width: 40rpx;
  height: 40rpx;
}
.addressMsg{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  flex: 1;
  margin-top: 16rpx;
}
.addressMsg view>text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:22rpx;
  color:rgba(153,153,153,1);
}
.addressMsg view>text:first-child{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:34rpx;
  color:rgba(51,51,51,1);
  display: inline-block;
  margin-right: 10rpx;
}
.addressMsg>text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:28rpx;
  color:rgba(102,102,102,1);
}
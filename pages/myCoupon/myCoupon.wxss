.box{
  border-top: 1px solid rgba(204,204,204,1);
  /* background:rgba(245,245,245,1); */
  /* padding-top: 24rpx; */
  padding-bottom: 88rpx;
  padding-top: 14rpx;
  height: 100vh;
  box-sizing: border-box;
}
.background{
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 14rpx rgba(204,204,204,0.2);
  border-radius:16rpx;
  padding: 28rpx;
  width: 686rpx;
  box-sizing: border-box;
  margin: 12rpx auto;
  /* margin-bottom: 24rpx; */
}
.weui-tabs{
  overflow: hidden;
  height: 70rpx;
}
.weui-tabs-bar__item{
  width: 50%;
  text-align: center;
  /* background:rgba(245,245,245,1) !important; */
  /* border: 2px solid rgba(245,245,245,1); */
}

.weui-tabs-bar__title,.tab-bar-title{
  font-size: 28rpx !important;
  padding-bottom: 10rpx !important;
}
.tab-content{
  display: flex;
}
.tab-bar-title__selected {
  color: #57D679;
}
.swiperClass{
  height: 82vh;
}
.coupon{
  width: 686rpx;
  height: 164rpx;
  position: relative;
  margin: 12rpx auto;
}
.coupon image{
  width: 100%;
  height: 100%;
}
.failure .moneyAndName{
  color: #999999 !important;
}
.failure .moneyAndName{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 28rpx;
}
.moneyAndName{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 28rpx;
  height:40rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  color:rgba(255,151,73,1);
  display: flex;
}
.moneyAndName>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.moneyAndName>view>text{
  font-size:24rpx;
}
.failure .moneyAndName .num{
  color:rgba(153,153,153,1);
}
.moneyAndName .num{
  font-size:44rpx;
  color:rgba(255,151,73,1);
  display: inline-block;
  margin-right: 35rpx;
  max-width: 105rpx;
}
.failure .time{
  color:rgba(153,153,153,1);
}
.time{
  height:28rpx ;
  /* position: absolute;
  top: 98rpx;
  left: 32rpx; */
  font-size:20rpx !important;
  font-family:PingFang SC;
  font-weight:500;
  line-height:28rpx;
  color:rgba(255,151,73,1);
}
.failure .getIt{
  color:rgba(153,153,153,1);
}
.getIt{
  position: absolute;
  right: 58rpx;
  top: 50%;
  transform: translateY(-50%);
  height:40rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  line-height:40rpx;
  color:rgba(255,151,73,1);
}
.canGet{
  right: 30rpx !important;
}
.box{
  background:rgba(245,245,245,1);
  padding-top: 24rpx;
  box-sizing: border-box;
  height: 100vh;
}
.background{
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 14rpx rgba(204,204,204,0.2);
  border-radius:16rpx;
  padding: 28rpx;
  width: 686rpx;
  box-sizing: border-box;
  margin: 0 auto;
  margin-bottom: 24rpx;
}
.form view{
  display: flex;
  border-bottom: 1px solid rgba(197,197,197,0.5019607843137255);
  height: 94rpx;
  align-items: center;
}
.last{
  border: none !important;
}
.form text{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:40rpx;
  display: inline-block;
  width: 130rpx;
  color:rgba(51,51,51,1);
}
.form input,.map{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:40rpx;
  color:rgba(153,153,153,1);
}
.picker{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:40rpx;
  color:rgba(102,102,102,1);
}
.add{
  margin-top: 30rpx;
  width: 560rpx !important;
  height: 88rpx;
  background:rgba(87,214,121,1);
  opacity:1;
  border-radius:44rpx;
  color: white;
  font-size:32rpx;
  font-family:PingFang SC;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 !important;
  font-weight:400;
}
.btn{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.btn button{
  display: flex;
  align-items: center;
  justify-content: center;
}
.delate{
  width:264rpx !important;
  height:88rpx;
  border:1px solid rgba(153,153,153,1);
  border-radius:46rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:44rpx;
  color:rgba(153,153,153,1);
  background: white;
}
.save{
  width:264rpx !important;
  height:88rpx;
  border:1px solid #57D679;
  background:rgba(87,214,121,1);
  border-radius:46rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:44rpx;
  color:#FFFFFF;
}
.setMoren{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.setMoren text{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  line-height:40rpx;
  color:rgba(51,51,51,1);
}
.setMoren switch{
  width: 102rpx;
}